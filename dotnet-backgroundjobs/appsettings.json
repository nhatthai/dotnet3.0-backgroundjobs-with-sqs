{
  "ConnectionString": "Host=localhost;Database=webapp;Username=postgres_user;Password=Your_password123",
  "Redis": "localhost:6379",
  "AWS": {
    "Profile": "development",
    "Region": "ap-southeast-1"
  },

  "Authentication": {
    "Cognito": {
      "ClientId": "<Client ID>",
      "IncludeErrorDetails": true,
      "MetadataAddress": "https://cognito-idp.<region-name>.amazonaws.com/<Pool ID>/.well-known/openid-configuration",
      "RequireHttpsMetadata": false,
      "ResponseType": "code",
      "SaveToken": true,
      "TokenValidationParameters": {
        "ValidateIssuer": true
      },
      "Authority": "https://cognito-idp.<region-name>.amazonaws.com/<Pool ID>"
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
